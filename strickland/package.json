{
  "name": "strickland",
  "version": "2.0.1",
  "description": "Strickland is a JavaScript validation framework with a focus on extensibility and composition",
  "main": "./lib/strickland.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run lint && npm test && babel src -d lib",
    "lint": "eslint src/** test/**",
    "test": "jest --verbose --coverage",
    "test:watch": "jest --watch",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffhandley/strickland.git"
  },
  "keywords": [
    "validation",
    "universal",
    "framework",
    "validator",
    "validate",
    "react",
    "redux",
    "flux",
    "node"
  ],
  "author": "Jeff Handley <npm@jeffhandley.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffhandley/strickland/issues"
  },
  "homepage": "http://strickland.io",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "deep-freeze": "0.0.1",
    "eslint": "^4.13.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.3"
  }
}
